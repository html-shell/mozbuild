{
  "cmd": "{PROJECT_PATH}/../Release/_virtualenv/Scripts/python.exe",
  "name": "MozBuild",
  "args": [ "{PROJECT_PATH}/../Release/config.status", "--backend=VisualStudio"],
  "sh": false,
  "cwd": "{PROJECT_PATH}/../Release/",
  "env": {
    "VARIABLE1": "VALUE1",
    "VARIABLE2": "VALUE2"
  },
  "errorMatch": "^(?<file>[^\\.]+.c):(?<line>\\d+):(?<col>\\d+).$",
  "targets": {
    "Release": {
      "cmd": "{PROJECT_PATH}/../mozilla-build/python/python",
      "sh": false,
      "args": [ "{PROJECT_PATH}/../../build/BoltBuild.py", "build-backend"]
    },
    "Build": {
      "cmd": "{PROJECT_PATH}/../mozilla-build/python/python",
      "sh": false,
      "args": [ "{PROJECT_PATH}/../../build/BoltBuild.py", "build"]
    }
  }
}
